-- Creating tables -- DDL

CREATE TABLE "students" (
"id" SERIAL, 
"name" TEXT,
"rank" INTEGER
);

-- \d students para ver los tipos de las columnas

CREATE TABLE "employees" (
  "id" SERIAL,
  "emp_name" TEXT,
  "manager_id" INTEGER
);

CREATE TABLE "employee_phones" (
  "emp_id" INTEGER,
  "phone_number" TEXT
);

-- Data Types

-- https://www.postgresql.org/docs/9.6/datatype.html